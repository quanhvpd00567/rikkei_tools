<div class="container">
  <div class="row">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">No.</th>
          <th scope="col">Employee Name</th>
          <th scope="col">Role</th>
          <th scope="col">Level</th>
        </tr>
      </thead>
      <tbody>
      <%- @employees.each do |employee, index|  %>
        <tr>
          <th scope="row"><%=employee.code %></th>
          <td><%= employee.name %></td>
          <td><%= employee.role %></td>
          <td><%= employee.level %></td>
          <td>
            <%= link_to "Edit", edit_project_path(employee) %>
          </td>
        </tr>
        <%- end %>
      </tbody>
    </table>
  </div>
</div>


